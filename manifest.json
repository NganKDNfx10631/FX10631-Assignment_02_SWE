         "js": [
            "script/libary/jquery.js",
            "script/lib/getData.js",
            "./script/lib/requestData.js",
            "script/libary/jquery-confirm.min.js",
            "script/lib/notifycation.js",
            "./script/subtitle/lib/subtitleHandling.js",
